			<plugin>
				<groupId>com.google.code.maven-replacer-plugin</groupId>
				<artifactId>maven-replacer-plugin</artifactId>
			</plugin>

			<!--
			 - Workaround for a shortcoming in the Groovy compiler.
			 - The Groovy stub generator sets the modified date of all generated files to the
			 - 1st of January 1970 in order to prevent the Java compiler from overwriting the
			 - groovy .class files with new ones based on the stub.
			 - However, adding @SuppressWarnings annotations to all generated code in the
			 - maven-replacer-plugin above overrides that and sets the modified date to the
			 - current time.
			 - Here we put the modified time back to 1970 to make the Groovy compiler happy
			 - again.
			 -
			 - See http://jira.codehaus.org/browse/MGROOVY-187 for the original Groovy defect.
			 -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>touch-modified-groovy-stubs</id>
						<phase>process-sources</phase>
						<configuration>
							<target>
								<touch datetime="1970-01-01T00:00:00 GMT" pattern="yyyy-MM-dd'T'HH:mm:ss z">
									<fileset dir="target/generated-sources/groovy-stubs" />
								</touch>
							</target>
						</configuration>
						<goals><goal>run</goal></goals>
					</execution>
				</executions>
			</plugin>

			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>openjpa-maven-plugin</artifactId>
			</plugin>

			<plugin>
				<artifactId>maven-surefire-plugin</artifactId>
