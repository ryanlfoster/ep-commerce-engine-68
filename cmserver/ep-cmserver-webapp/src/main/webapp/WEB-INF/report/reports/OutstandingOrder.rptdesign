            <property name="dataSource">EpScriptedDataSource</property>
            <method name="open"><![CDATA[var orderService = new Packages.com.elasticpath.cmweb.reportservice.impl.OrderReportServiceImpl();

var startdate;
if (params["startDate"].value == null) {
	startdate = null;
} else {
	startdate = params["startDate"];
}
orders = orderService.outstandingOrderSummaryReport(startdate);
totalRows = orders.size();


currentRow = 0;]]></method>
            <method name="fetch"><![CDATA[if (currentRow >= totalRows) {
    return false;
}

var order = orders.get(currentRow);

currentRow++;
row["CreatedDate"] = order[0];
row["OrderNo"] = order[1];
row["CustName"] = order[2];
row["CustEmail"] = order[3];
row["TotalSales"] = order[4];
row["TotalPayment"] = order[5];
row["ShipStatus"] = order[6];
row["OrderStatus"] = order[7];


return true;]]></method>
            <method name="close"><![CDATA[orderService = null;
orders = null;

totalRows = null;
currentRow = null;]]></method>
