            <method name="open"><![CDATA[customerService = new Packages.com.elasticpath.cmweb.reportservice.impl.CustomerReportServiceImpl();

var startDate;
var endDate;

if (params["startDate"].value == null) {
	startDate = null;
} else {
	startDate = params["startDate"];
}
if (params["endDate"].value == null) {
	endDate = null;
} else {
	endDate = params["endDate"];
}

/* FOR TESTING PURPOSES ONLY
engine = new Packages.com.elasticpath.persistence.TestPersistenceEngine();
customerService.setPersistenceEngine(engine);
*/


customers = customerService.registrationReport(startDate, endDate);

totalRows = customers.size();

currentRow = 0;]]></method>
            <method name="fetch"><![CDATA[if (currentRow >= totalRows) {
    return false;
}

var customer = customers.get(currentRow);

currentRow++;
row["createDate"] = customer[0];
row["uidPk"] = new Number(customer[1]);
row["firstName"] = customer[2];
row["lastName"] = customer[3];
row["email"] = customer[4];
row["anonymous"] = customer[5];
row["phoneNumber"] = customer[6];

var stringDate = new String(row["createDate"]);
row["yearMonthDay"] = stringDate.substring(11, 15) + " " + stringDate.substring(4, 7) + " " +  
	+ stringDate.substring(8, 10);

return true;]]></method>
            <method name="close"><![CDATA[customerService = null;
customers = null;

totalRows = null;
currentRow = null;]]></method>
