            <method name="open"><![CDATA[orderService = new Packages.com.elasticpath.cmweb.reportservice.impl.OrderReportServiceImpl();

/* FOR TESTING PURPOSES ONLY
ep = new Packages.com.elasticpath.persistence.TestElasticPath();
engine = new Packages.com.elasticpath.persistence.TestPersistenceEngine();
orderService.setPersistenceEngine(engine);
*/

var startDate;
var endDate;

if (params["startDate"].value == null) {
	startDate = null;
} else {
	startDate = params["startDate"];
}
if (params["endDate"].value == null) {
	endDate = null;
} else {
	endDate = params["endDate"];
}

orders = orderService.orderSummaryReport(startDate, endDate, params["cancelled"]);
totalRows = orders.size();

currentRow = 0;]]></method>
            <method name="fetch"><![CDATA[if (currentRow >= totalRows) {
    return false;
}

var order = orders.get(currentRow);

currentRow++;
row["orderCount"] = order[0];
row["createDate"] = order[1];
row["totalTax"] = order[2];
row["totalDiscount"] = order[3];
row["total"] = order[4];
row["currency"] = order[5];
row["productCount"] = order[6];

var stringDate = new String(row["createDate"]);
row["yearMonthCurrency"] = stringDate.substring(4, 7) + " " + stringDate.substring(11, 15) 
	+ " (in " + row["currency"] + ")";

return true;]]></method>
            <method name="close"><![CDATA[orderService = null;
orders = null;

totalRows = null;
currentRow = null;]]></method>
