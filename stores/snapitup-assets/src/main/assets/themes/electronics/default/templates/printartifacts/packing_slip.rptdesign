            <method name="fetch"><![CDATA[if (orderShipmentCurrentRow >= orderShipmentTotalRows) {
    return false;
}

var orderShipment = orderShipments.get(orderShipmentCurrentRow);

orderShipmentCurrentRow++;
row["billingName"]     = orderShipment[0]  + " " + orderShipment[1];
var additionalBillingAddress = orderShipment[3];
if (additionalBillingAddress==null || additionalBillingAddress=="") {
	row["billingAddress"]  = orderShipment[2];	
} else {
	row["billingAddress"]  = orderShipment[2]  + " / " + additionalBillingAddress;
}
row["billingZipInfo"]  = orderShipment[4]  + " " + orderShipment[5];
row["billingCountry"]  = orderShipment[6];
row["shippingName"]    = orderShipment[7]  + " " + orderShipment[8];
var additionalShippingAddress = orderShipment[10];
if (additionalShippingAddress==null || additionalBillingAddress=="") {
	row["shippingAddress"] = orderShipment[9]; 
} else {
	row["shippingAddress"] = orderShipment[9]  + " / " + additionalShippingAddress; 
}
row["shippingZipInfo"] = orderShipment[11] + " " + orderShipment[12]; 
row["shippingCountry"] = orderShipment[13];
row["orderDate"] = orderShipment[14];
row["orderNumber"] = orderShipment[15];
row["itemSubtotal"] = orderShipment[16];
row["shippingCost"] = orderShipment[17];
row["subtotalDiscount"] = orderShipment[18];
row["itemTax"] = orderShipment[19];
row["shippingTax"] =  orderShipment[20];
row["shipmentNumber"] = orderShipment[21];
row["totalBeforeTax"] =  orderShipment[23];
row["total"] =  orderShipment[24];
row["storeName"] = orderShipment[25];

return true;]]></method>
