    <method name="beforeFactory"><![CDATA[packingSlipReportService = new Packages.com.elasticpath.cmclient.warehouse.packingslipreport.service.impl.PackingSlipReportServiceImpl();

var orderShipmentNumber;

if (params["orderShipmentNumber"].value == null) {
	orderShipmentNumber = null;
} else {
	orderShipmentNumber = params["orderShipmentNumber"];
}

java.lang.System.out.println("Parameter Debug: " + orderShipmentNumber);

orderShipments = packingSlipReportService.orderShipmentReport(orderShipmentNumber);

orderSkus = packingSlipReportService.orderSkuReport(orderShipmentNumber);

orderShipmentTotalRows = orderShipments.size();

orderShipmentCurrentRow = 0;

orderSkuTotalRows = orderSkus.size();

orderSkuCurrentRow = 0;]]></method>
